{
  "manifest_version": 2,
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "content-script.js"
    ],
    "run_at": "document_end"
  }],
  "name": "voldetrump",
  "description": "converts the word 'Trump' to 'Voldemort' on any webpage",
  "version": "1.0.1",
  "author": "Devin Frenze"
}